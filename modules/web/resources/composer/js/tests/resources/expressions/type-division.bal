function intDivideByFloat(int a, float b) (float) {
    return a / b;
}

function floatDivideByInt(float a, int b) (float) {
    return a / b;
}

function intGTFloat(int a, float b) (boolean) {
return a > b;
}

function floatGTInt(float a, int b) (boolean) {
return a > b;
}

function intMultiplyByFloat(int a, float b) (float) {
    return a * b;
}

function floatMultiplyByInt(float a, int b) (float) {
    return a * b;
}

function intAddFloat(int a, float b) (float) {
    return a + b;
}

function floatAddInt(float a, int b) (float) {
    return a + b;
}

function intSubtractFloat(int a, float b) (float) {
