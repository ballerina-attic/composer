function intLTFloat(int a, float b) (boolean) {
    return a < b;
}

function floatLTInt(float a, int b) (boolean) {
    return a < b;
}

function intLTEFloat(int a, float b) (boolean) {
    return a <= b;
}

function floatLTEInt(float a, int b) (boolean) {
